<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <_ReportPortal_XUnit_Runtime_Path>$(MSBuildThisFileDirectory)..\..\lib\net462\ReportPortal.XUnit.V3.reporters.dll</_ReportPortal_XUnit_Runtime_Path>
    <_ReportPortal_XUnit_Runtime_Pdb_Path>$(MSBuildThisFileDirectory)..\..\lib\net462\ReportPortal.XUnit.V3.reporters.pdb</_ReportPortal_XUnit_Runtime_Pdb_Path>
  </PropertyGroup>

  <ItemGroup>
    <None Include="$(_ReportPortal_XUnit_Runtime_Path)" >
      <Link>%(Filename)%(Extension)</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>False</Visible>
    </None>
    
    <None Include="$(_ReportPortal_XUnit_Runtime_Pdb_Path)" >
      <Link>%(Filename)%(Extension)</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>False</Visible>
    </None>

    <None Include="$(_ReportPortal_Shared_Runtime_Path)" Condition="'$(_ReportPortal_Shared_Runtime_Path)' != ''">
      <Link>%(Filename)%(Extension)</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>False</Visible>
    </None>
  </ItemGroup>

</Project>
